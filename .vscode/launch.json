    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Run review",
            "type": "python",
            "request": "launch",
            "module": "pr_agent.cli",
            "justMyCode": false,
            "args": ["--pr_url", "https://dev.azure.com/yochailado/PRfect/_git/pr-agent.git/pullrequest/1", "review"]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}